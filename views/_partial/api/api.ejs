<script type="text/javascript">
  let tabGPS = [];
  <% if (locals.user.gps) {%>
    <%  for (const gps of locals.user.gps) {%>
      var gps = new Object();
      gps.altitude = <%=gps.altitude%>;
      gps.longitude = <%=gps.longitude%>;
      gps.latitude = <%=gps.latitude%>;
      console.log(gps)
      tabGPS.push(gps);
    <%}%>
  <%} %>

console.log(tabGPS);


</script>
